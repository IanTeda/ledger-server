{
  "name": "ledger-server",
  "version": "1.0.0",
  "description": "API server for personal ledger",
  "main": "app.js",
  "scripts": {
    "dev:db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "dev:db:migrate": "npx sequelize-cli db:migrate",
    "dev:start": "clear; nodemon --exec babel-node server/app.js",
    "start": "babel-node server/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "slugify": "^1.4.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "faker": "^4.1.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "webpack": "^4.43.0"
  }
}
